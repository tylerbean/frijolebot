{
  "name": "discord-link-bot",
  "version": "1.0.0",
  "description": "Discord bot that monitors channels for links and stores them in PostgreSQL for management",
  "main": "discord-link-bot.js",
  "scripts": {
    "start": "node discord-link-bot.js",
    "dev": "nodemon discord-link-bot.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "node --experimental-vm-modules node_modules/.bin/jest --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "test:ci": "node --experimental-vm-modules node_modules/.bin/jest --ci --coverage --watchAll=false"
  },
  "keywords": [
    "discord",
    "bot",
    "postgresql",
    "link-monitoring",
    "kubernetes",
    "health-checks"
  ],
  "author": "Tyler Bean",
  "license": "MIT",
  "dependencies": {
    "@whiskeysockets/baileys": "^7.0.0-rc.2",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
